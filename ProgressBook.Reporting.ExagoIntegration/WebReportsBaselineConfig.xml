  <function>
    <name>MonthName</name>
    <description>Returns the name of the month. Function can take a formatted date string, a Date or DateTime value, or an integer value as input</description>
    <category>Date</category>
    <min_args>1</min_args>
    <max_args>1</max_args>
    <arguments_json>[{"Name":"inputDateOrNumber","Required":true,"Description":"A formatted date string, a Date or DateTime value, or an integer value"}]</arguments_json>
    <variable_arguments>false</variable_arguments>
    <available_in>1</available_in>
    <filter_return_type>String</filter_return_type>
    <weight>101</weight>
    <language>cs</language>
    <program_code>
      //create return value
      string monthName = "";

      //create int to hold month num
      int monthNumber;

      //create datetime to hold date
      DateTime myDate;

      //see if input is an integer or a date
      bool intCheck = Int32.TryParse(args[0].ToString(), out monthNumber);
      bool dateCheck = DateTime.TryParse(args[0].ToString(), out myDate);

      //make appropriate conversion based on input type
      if(intCheck &amp;&amp; monthNumber &gt; 0 &amp;&amp; monthNumber &lt; 13)
      monthName = System.Globalization.CultureInfo.CurrentCulture.DateTimeFormat.GetMonthName(monthNumber);
      else if(dateCheck)
      monthName = System.Globalization.CultureInfo.CurrentCulture.DateTimeFormat.GetMonthName(myDate.Month);
      else
      monthName = "Conversion failed. Please check function input.";

      //return converted month name or failure message
      return monthName;
    </program_code>
    <reference>
      <name>mscorlib.dll</name>
    </reference>
  </function>
