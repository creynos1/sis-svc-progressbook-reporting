  <function>
    <name>EmisAssessmentValue</name>
    <category>Other</category>
    <min_args>1</min_args>
    <max_args>3</max_args>
    <available_in>1</available_in>
    <filter_return_type>String</filter_return_type>
    <weight>0</weight>
    <language>CSharp</language>
    <program_code>
      // Takes a series of EMIS assessment scores (which may include asterisks ***) and determines the correct value to report.
      // Parameter args (array of objects) should be defined by caller in order of precedence, highest first.
      // These params may be of type string, int? or decimal?.
      bool anyAsterisks = false;
      for(int i=0;i &lt; args.Length; i++) {
      if (args[i] == null) continue;
      string val = args[i].ToString().Trim();
      bool hasAsterisks =  val == "***";
      anyAsterisks = anyAsterisks || hasAsterisks;
      if (val != "" &amp;&amp; !hasAsterisks) {
      return val;
      }
      }
      return (anyAsterisks ? "***" : null);
    </program_code>
  </function>
