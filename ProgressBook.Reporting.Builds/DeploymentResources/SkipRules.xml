  <SkipRule Name="SkipExagoWebReportsXmlEnc" ObjectName="filePath" SkipAction="Update" AbsolutePath=".*\\Exago\\Config\\WebReports.xml.enc$" />
  <SkipRule Name="SkipDeleteCustomFolder" ObjectName="dirPath" SkipAction="Delete" AbsolutePath=".*\\Custom$"/>
  <SkipRule Name="SkipDeleteCustomSubFolderContents" ObjectName="dirPath" SkipAction="Delete" AbsolutePath=".*\\Custom\\.*"/>
  <SkipRule Name="SkipUpdateCustomFolderContents" ObjectName="filePath" SkipAction="Update" AbsolutePath=".*\\Custom\\.*"/>
  <SkipRule Name="SkipDeleteCustomFolderContents" ObjectName="filePath" SkipAction="Delete" AbsolutePath=".*\\Custom\\.*"/>
</SkipRules>